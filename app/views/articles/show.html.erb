<h1>See the Article</h1>


<h2>Article: <%= @article.title%> - Content: <%=@article.content %>  </h2>



<div></div>

<button type='button' class='btn btn-outline-primary' style='color:white;'> <%= link_to "Back to articles", articles_path %> </button>
<button type='button' class='btn btn-outline-primary'><%= link_to "Edit Article", edit_article_path(@article) %> </button>
<button type='button' class='btn btn-outline-primary'><%= link_to "Delete Article", article_path(@article), method: :delete, data: {confirm: "Right about it?"} %> </button>

